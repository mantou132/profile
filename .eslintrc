{
    "root": true,
    "rules": {
        "indent": [
            2,
            4
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "prefer-const": 0,
        "vars-on-top": 0,
        "func-names": 0,
        "strict": [2, "global"],
        "spaced-comment": [0, "always"],
        "no-param-reassign": [1, {"props": true}],
        "no-shadow": 0,
        "dot-notation": 1,
        "no-extra-parens": 1,
        "no-console": 2,
        "no-else-return": 0,
    },
    "env": {
        "node": true
    },
    "globals": {
        include: false
    },
    "extends": "airbnb/legacy"
}
